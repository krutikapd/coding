#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	long int t;
	cin>>t;
	while(t--)
	{
	    long int n,k;
	    cin>>n>>k;
	    long int ans=0;
	    int num[n];
	    for(int i=0;i<n;i++)
	       cin>>num[i];
	    for(int i=0;i<n;i++)
	    {
	        if(num[i]>k)
	          ans+=(num[i]-k);
	    }
	    cout<<ans<<endl;
	}
	
	return 0;
}
